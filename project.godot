; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="2048"
run/main_scene="res://game/jogo.tscn"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://gnome-2048-icon.png"

[autoload]

data="*res://save/datat.gd"

[display]

window/size/width=600
window/size/height=1024
window/size/test_width=300
window/size/test_height=512
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="expand"

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

environment/default_clear_color=Color( 0.470588, 0.458824, 0.415686, 1 )
environment/default_environment="res://default_env.tres"
